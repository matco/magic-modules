type <%= product_name -%>OperationWaiter struct {
  CommonOperationWaiter,
  Config *Config
}

func (w *<%= product_name -%>OperationWaiter) QueryOp() (interface{}, error) {
  if w == nil {
    return nil, fmt.Errorf("Cannot query operation, it's unset or nil.")
  }
  // Returns the proper get.
  return sendRequestWithTimeout(w.Config, "GET", w.Op.Name, nil, 4)
}

func <%= product_name.underscore -%>OperationWaitTime(config *Config, op *Operation, project, activity, timeoutMinutes int) error {
  w := &<%= product_name -%>OperationWaiter{
    Config: config,
  }
  if err := w.SetOp(op); err != nil {
    return err
  }
  return OperationWait(w, activity, timeoutMinutes)
}
